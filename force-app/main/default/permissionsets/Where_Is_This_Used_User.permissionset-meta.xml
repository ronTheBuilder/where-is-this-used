<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <label>Where Is This Used? User</label>
    <description>Grants access to the Where Is This Used? app, tab, Apex classes, and custom permission. Assign to admins and developers who need metadata dependency analysis.</description>
    <applicationVisibilities>
        <application>Where_Is_This_Used</application>
        <visible>true</visible>
    </applicationVisibilities>
    <customPermissions>
        <enabled>true</enabled>
        <name>WITU_Access</name>
    </customPermissions>
    <classAccesses>
        <apexClass>DependencyService</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>DependencyController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>MetadataPickerController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <tabSettings>
        <tab>Where_Is_This_Used</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <hasActivationRequired>false</hasActivationRequired>
</PermissionSet>
